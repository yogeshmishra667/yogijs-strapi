{
  "kind": "collectionType",
  "collectionName": "user_details",
  "info": {
    "singularName": "user-detail",
    "pluralName": "user-details",
    "displayName": "user-details"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "minLength": 3,
      "maxLength": 30,
      "required": true
    },
    "email": {
      "type": "email",
      "required": true,
      "minLength": 10,
      "unique": true
    },
    "subject": {
      "type": "string"
    },
    "message": {
      "type": "text"
    }
  }
}
